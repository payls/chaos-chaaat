{
  "name": "webapp_admin",
  "version": "0.0.1",
  "description": "Chaaat Application",
  "main": "server.js",
  "engines": {
    "node": "20.13.1"
  },
  "scripts": {
    "lint": "next lint",
    "start": "next start -p $PORT",
    "start:production": "yarn install && NODE_ENV=production next build && next start -p $PORT",
    "start:dev": "NODE_ENV=development next dev --turbo -p 3112",
    "build:dev": "NODE_ENV=development & next build --debug",
    "start:generic": "node_modules/next/dist/bin/next build && node_modules/next/dist/bin/next start -p $PORT",
    "start:nobuild": "node_modules/next/dist/bin/next start -p $PORT",
    "start:test": "yarn install && NODE_ENV=staging next build && next start -p 3115",
    "docker-compose:up": "cd build/docker && docker compose up -d",
    "docker-compose:down": "cd build/docker && docker compose down"
  },
  "repository": "https://bitbucket.org/pave/webapp_admin",
  "author": "Mervin Tan <mervin@yourpave.com>",
