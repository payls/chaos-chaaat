{
  "name": "pave_job_api_workflows",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "20.13.1"
  },
  "scripts": {
    "start:production": "NODE_ENV=production node server.js",
    "start:dev": "NODE_ENV=development node --inspect --trace-warnings server.js -e less,js,css,json,hbs",
    "start:generic": "npm ci && node server.js",
    "test": "IS_TEST=1 NODE_ENV=development mocha --exit --timeout 15000 -R spec server/tests/**/*.js",
    "ups": "sequelize db:migrate && sequelize db:seed:all",
    "downs": "sequelize db:migrate:undo:all",
    "down": "sequelize db:migrate:undo",
    "apidoc": "apidoc -i ./server/routes/ -o ./public/docs/api/",
    "lint": "npx eslint server/**/*.js",
    "docker-compose:up": "cd build/docker && docker-compose up -d",
    "docker-compose:down": "cd build/docker && docker-compose down",
    "load": "node -r dotenv/config server.js",
    "start:pm2-development": "pm2 start ecosystem.config.js --env development",
    "start:pm2-staging": "pm2-runtime ecosystem.config.js --env staging",
    "start:pm2-production": "pm2-runtime ecosystem.config.js --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://johnyourpave@bitbucket.org/pave/pave_job_api_workflows.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/pave/pave_job_api_workflows#readme",
