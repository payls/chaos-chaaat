    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@hubspot/api-client": "^11.2.0",
    "@sentry/node": "^8.51.0",
    "@smithy/protocol-http": "^4.0.0",
    "@smithy/signature-v4": "^3.0.0",
    "@wix/crm": "^1.0.142",
    "@wix/pricing-plans": "^1.0.101",
    "@wix/sdk": "^1.14.4",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "aws-sdk": "^2.1625.0",
