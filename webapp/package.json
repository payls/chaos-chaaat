{
  "name": "webapp",
  "version": "0.0.1",
  "description": "Pave Webapp",
  "main": "server.js",
  "engines": {
    "node": "14.19.1"
  },
  "scripts": {
    "start": "next start -p $PORT",
    "start:production": "npm install && NEXT_PUBLIC_APP_ENV=production next build && next start -p $PORT",
    "start:dev": "NEXT_PUBLIC_APP_ENV=development next dev -p 3111",
    "start:generic": "NODE_OPTIONS='-r @newrelic/next' node_modules/next/dist/bin/next start -p $PORT",
    "start:test": "npm install && NEXT_PUBLIC_APP_ENV=staging next build && next start -p 3115",
    "start:generic-dev": "NEXT_PUBLIC_APP_ENV=development next build && next start -p 3111",
    "docker-compose:up": "cd build/docker && docker-compose up -d",
    "docker-compose:down": "cd build/docker && docker-compose down",
    "cypress": "cypress open",
    "test": "NEXT_PUBLIC_APP_ENV=staging start-server-and-test start:test http://localhost:3115 cypress",
    "test:ci": "NEXT_PUBLIC_APP_ENV=staging npx next build && npx next start -p 3115 & npx wait-on http://localhost:3115 && npx cypress run --config video=false",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "repository": "https://bitbucket.org/pave/webapp",
  "author": "Mervin Tan <mervin@yourpave.com>",
  "dependencies": {
    "@charkour/react-reactions": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@newrelic/next": "^0.1.1",
    "@next/bundle-analyzer": "^13.0.4",
    "@react-google-maps/api": "^2.1.1",
    "@react-oauth/google": "^0.12.1",
    "@react-pdf-viewer/core": "^3.6.0",
    "@react-pdf-viewer/default-layout": "^3.6.0",
    "@reactchartjs/react-chart.js": "^1.0.0-rc.3",
    "@tanem/react-nprogress": "^5.0.1",
    "@tanstack/react-query": "4.16.1",
    "animate.css": "^4.1.1",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.4",
    "cross-env": "^7.0.3",
    "cypress": "^11.1.0",
    "fastify-jwt": "^2.1.3",
    "flag-icons": "^6.6.6",
